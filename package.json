{
  "name": "poe-build-list",
  "version": "1.1.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "npm run test -- --watch",
    "test-client": "npm test --prefix src/client",
    "client": "npm start --prefix src/client",
    "server": "nodemon src/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "generate-tags": "node ./scripts/generateTags.js",
    "generate-tags-save": "node ./scripts/generateTags.js ./src/lib/tags.json",
    "install-all": "npm install && npm install --prefix src/client",
    "prod": "npm run install-all && npm run build --prefix src/client"
  },
  "author": "Patrick Mackow",
  "license": "ISC",
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.0",
    "prettier": "^1.19.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/client/"
    ]
  }
}

{
  "name": "poe-build-list",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "npm run test -- --watch",
    "test-client": "npm test --prefix src/client",
    "client": "npm start --prefix src/client",
    "server": "nodemon src/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "generate-tags": "node ./scripts/generateTags.js",
    "install-all": "npm install && npm install --prefix src/client",
    "prod": "npm run install-all && npm run build --prefix src/client"
  },
  "author": "Patrick Mackow",
  "license": "ISC",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.16",
    "prettier": "^1.13.7"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/client/"
    ]
  }
}
